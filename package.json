{
  "name": "nimjs",
  "version": "1.0.0",
  "description": "ITB Nim Finder for internal use",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npm run runserver",
    "build": "npm run build-ts",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "runserver": "node dist/server/server.js",
    "watchserver": "nodemon --watch '**/*' --ignore '**/*.spec.ts' --exec 'ts-node' server/server.ts"
  },
  "repository": {
    "type": "git",
    "url": "github.com/jauhararifin/nimjs"
  },
  "author": "Jauhar Arifin",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.18.3",
    "cheerio": "^1.0.0-rc.2",
    "compression": "^1.7.2",
    "dotenv": "^6.0.0",
    "errorhandler": "^1.5.0",
    "express": "^4.16.3",
    "express-validator": "^5.2.0",
    "mongoose": "^5.1.4",
    "mongoose-auto-increment": "^5.0.1",
    "morgan": "^1.9.0",
    "superagent": "^3.8.3"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/cheerio": "^0.22.7",
    "@types/compression": "0.0.36",
    "@types/debug": "0.0.30",
    "@types/dotenv": "^4.0.3",
    "@types/errorhandler": "0.0.32",
    "@types/express": "^4.16.0",
    "@types/express-validator": "^3.0.0",
    "@types/mongoose": "^5.0.15",
    "@types/mongoose-auto-increment": "^5.0.30",
    "@types/morgan": "^1.7.35",
    "@types/superagent": "^3.8.0",
    "nodemon": "^1.17.5",
    "ts-node": "^6.1.0",
    "tslint": "^5.10.0",
    "typescript": "^2.9.1"
  }
}
